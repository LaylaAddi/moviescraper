
<% @movies.each do |movie| %> 
    <div class="box panel panel-default">
      <%= link_to image_tag movie.image_url %>
      <h2>
        <%= link_to  movie.title, movie %>
      </h2>
      <p class="user">
        Submitted by
        <%= movie.user.username %>
      </p>
    </div>
  <% end %>
